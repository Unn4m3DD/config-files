[
  {
    "key": "f5",
    "command": "-workbench.action.debug.continue",
    "when": "inDebugMode"
  },
  {
    "key": "f5",
    "command": "-workbench.action.debug.start",
    "when": "!inDebugMode"
  },
  {
    "key": "f5",
    "command": "workbench.action.tasks.reRunTask"
  },
  {
    "key": "ctrl+alt+f5",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "ctrl+e",
    "command": "cursorEndSelect",
    "when": "textInputFocus"
  },
  {
    "key": "shift+end",
    "command": "-cursorEndSelect",
    "when": "textInputFocus"
  }
]
