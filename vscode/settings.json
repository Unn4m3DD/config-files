{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true, //=>
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "workbench.colorTheme": "Dracula",
  "omnisharp.path": "latest",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "[csharp]": {
    "editor.defaultFormatter": "Leopotam.csharpfixformat"
  },
  "csharpfixformat.style.spaces.beforeParenthesis": false,
  "csharp.referencesCodeLens.enabled": false,
  "cSpell.userWords": [
    "Abdl",
    "Mathf",
    "READMEABDL",
    "Sena",
    "behaviour",
    "machine",
    "pawngame",
    "quadtree",
    "state"
  ],
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.linting.pylintArgs": [
    "--indent-string=\"  \"",
    "--ignore=\"E1124\""
  ],
  "python.formatting.autopep8Args": [
    "--indent-size=2",
    "redyu"
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "files.associations": {
    "*.html": "html",
    "*.pl": "prolog",
    "*.bdl": "bdl",
    "*.shader": "glsl"
  },
  "workbench.iconTheme": "material-icon-theme",
  "liveServer.settings.donotShowInfoMsg": true,
  "[markdown]": {
    "editor.defaultFormatter": "mervin.markdown-formatter"
  },
  "git.autofetch": true,
  "cSpell.language": "en,pt-PT",
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, DerivePointerAlignment: false, PointerAlignment: Left, ColumnLimit: 0}",
  "editor.rulers": [
    90
  ],
  "java.semanticHighlighting.enabled": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "explorer.confirmDelete": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "cmake.debugConfig": {
    "args": [
      "11.11.11.11",
      "55555",
      "1"
    ]
  },
  "python.languageServer": "Microsoft",
  "cmake.configureOnOpen": true,
  "C_Cpp.updateChannel": "Insiders",
  "angular.experimental-ivy": true,
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "terminal.integrated.tabs.enabled": true,
  "mssql.connections": [
    {
      "server": "{{put-server-name-here}}",
      "database": "{{put-database-name-here}}",
      "user": "{{put-username-here}}",
      "password": "{{put-password-here}}"
    }
  ],
  "window.zoomLevel": 1,
}